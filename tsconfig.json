{
    "compilerOptions": {
        "moduleResolution": "node",
        "module": "commonjs",
        "types": [
            "node",
            "@wdio/globals/types",
            "expect-webdriverio",
            "@wdio/cucumber-framework"
        ],
        "paths": {
            "@helpers/*": ["./src/helpers/*"],
            "@pageObjects/*": ["./src/pageObjects/*"],
            "@interfaces/*": ["./src/interfaces/*"]
          },
        "target": "es2022"
    }
}